# Environment Configuration
NODE_ENV=development
PORT=3001

# Rate Limiting
RATE_LIMIT_POINTS=100
RATE_LIMIT_DURATION=3600

# Security
FRONTEND_URL=http://localhost:3000

# Lenovo API Configuration (if available)
LENOVO_API_KEY=your_api_key_here
LENOVO_API_URL=https://api.lenovo.com

# Browser Configuration for Puppeteer
PUPPETEER_HEADLESS=true
PUPPETEER_TIMEOUT=30000

# Cache Configuration
CACHE_TTL=3600

# File Upload Limits
MAX_FILE_SIZE=5242880
MAX_SERIAL_NUMBERS=50

# Logging
LOG_LEVEL=info